 year = {2010}
}

@article{pub.1045923679,
 abstract = {Le crâne d'un éléphant à défense droite, Palaeoloxodon antiquus, a été découvert en 1989 à Gesher Benot Ya'aqov, site du Pleistocene moyen, en Israël. Ce crâne étroitement associé à des artefacts en pierre et en bois fait partie intégrante de la composition du sol d'occupation acheuléen. Son emplacement et les dommages qu'il a subis suggèrent des fractures intentionnelles pour l'extraction de la cervelle. Un nucleus en basalte, un bloc de pierre et un morceau de chêne se trouvaient sous le crâne et ont pu être utilisés pour le retourner. On décrit ici un scénario possible de chasse et de démembrement, en s' inspirant de parallèles archéologiques et modernes. Ces trouvailles corroborent d'autres éléments mis en évidence sur le site qui indiquent un comportement sophistiqué des habitants de cette région, il y a 500.000 ans.
A skull of a straight-tusked elephant, Palaeoloxodon antiquus, was discovered in 1989 at the Middle Pleistocene site of Gesher Benot Ya'aqov, Israel. The skull is closely associated with stone and wood artifacts, and is an integral component of an Acheulian living-floor. The specimen was positioned and damaged in such a way as to suggest deliberale breakage for brain extraction. A basalt core, a boulder and an oak log were found underneath the skull. These may have been used to invert the cranium. A possible hunting and dismemberment scenario is described, drawing on modern and archaeological parallels. These finds corroborate other evidence from the site, which point to sophisticated behaviour by the inhabitants of this region some 500,000 years ago.},
 author = {Goren-Inbar, Naama and Lister, A. and Werker, Ella and Chech, M.},
 doi = {10.3406/paleo.1994.4604},
 journal = {Paléorient},
 keywords = {},
 number = {1},
 pages = {99-112},
 title = {A Butchered elephant skull and associated artifacts from the Acheulian site of Gesher Benot Ya'Aqov, Israel},
 url = {https://nsf.dimensions.ai/details/publication/pub.1045923679},
 volume = {20},
 year = {1994}
}

