 number = {2},
 pages = {379-396},
 title = {KONSEP ISLAM TENTANG ADAT: Telaah Adat Dan 'Urf Sebagai Sumber Hukum Islam},
 url = {https://nsf.dimensions.ai/details/publication/pub.1144843417},
 volume = {9},
