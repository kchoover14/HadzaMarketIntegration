@article{pub.1045095526,
 abstract = {Disease susceptibility can arise as a consequence of adaptation to infectious disease. Recent findings have suggested that higher rates of chronic kidney disease (CKD) in individuals with recent African ancestry might be attributed to two risk alleles (G1 and G2) at the serum-resistance-associated (SRA)-interacting-domain-encoding region of APOL1. These two alleles appear to have arisen adaptively, possibly as a result of their protective effects against human African trypanosomiasis (HAT), or African sleeping sickness. In order to explore the distribution of potential functional variation at APOL1, we studied nucleotide variation in 187 individuals across ten geographically and genetically diverse African ethnic groups with exposure to two Trypanosoma brucei subspecies that cause HAT. We observed unusually high levels of nonsynonymous polymorphism in the regions encoding the functional domains that are required for lysing parasites. Whereas allele frequencies of G2 were similar across all populations (3%-8%), the G1 allele was only common in the Yoruba (39%). Additionally, we identified a haplotype (termed G3) that contains a nonsynonymous change at the membrane-addressing-domain-encoding region of APOL1 and is present in all populations except for the Yoruba. Analyses of long-range patterns of linkage disequilibrium indicate evidence of recent selection acting on the G3 haplotype in Fulani from Cameroon. Our results indicate that the G1 and G2 variants in APOL1 are geographically restricted and that there might be other functional variants that could play a role in HAT resistance and CKD risk in African populations. },
 author = {Ko, Wen-Ya and Rajan, Prianka and Gomez, Felicia and Scheinfeldt, Laura and An, Ping and Winkler, Cheryl A. and Froment, Alain and Nyambo, Thomas B. and Omar, Sabah A. and Wambebe, Charles and Ranciaro, Alessia and Hirbo, Jibril B. and Tishkoff, Sarah A.},
 doi = {10.1016/j.ajhg.2013.05.014},
 journal = {The American Journal of Human Genetics},
 keywords = {},
 note = {http://www.cell.com/article/S0002929713002279/pdf},
 number = {1},
 pages = {54-66},
 title = {Identifying Darwinian Selection Acting on Different Human APOL1 Variants among Diverse African Populations},
 url = {https://nsf.dimensions.ai/details/publication/pub.1045095526},
 volume = {93},
 year = {2013}
}

@article{pub.1045096199,
 abstract = {Meat scavenged by early Homo could have contributed importantly to a higher-quality diet. However, it has been suggested that because carrion would normally have been contaminated by bacteria it would have been dangerous and therefore eaten rarely prior to the advent of cooking. In this study, we quantified bacterial loads on two tissues apparently eaten by hominins, meat and bone marrow. We tested the following three hypotheses: (1) the bacterial loads on exposed surfaces of raw meat increase within 24 h to potentially dangerous levels, (2) simple roasting of meat on hot coals kills most bacteria, and (3) fewer bacteria grow on marrow than on meat, making marrow a relatively safe food. Our results supported all three hypotheses. Our experimental data imply that early hominins would have found it difficult to scavenge safely without focusing on marrow, employing strategies of carrion selection to minimize pathogen load, or cooking.},
 author = {Smith, Alex R. and Carmody, Rachel N. and Dutton, Rachel J. and Wrangham, Richard W.},
 doi = {10.1016/j.jhevol.2015.03.013},
 journal = {Journal of Human Evolution},
 keywords = {},
 number = {},
 pages = {62-70},
 title = {The significance of cooking for early hominin scavenging},
