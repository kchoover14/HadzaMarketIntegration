@inbook{pub.1030581256,
 abstract = {The microbiota is a collection of mostly bacterial species, with minor proportions of archaea and fungi. There is an extensive viral component in the form of bacteriophages, which parasitize the bacteria. The principal emergent activity of the colon microbiota is recycling of plant and animal materials that have been consumed by the host and have passed through the human digestive system. The bowel microbiota can be considered an organ of the body, with metabolic activity equivalent to that of the liver. This chapter considers some aspects of human physiology reflect the activities of the colon microbiota. The resiliency of the human bowel microbiota is certainly reassuring given the widespread use of antibiotics in humans. The relationship of the gut microbiota to human welfare, meanwhile, is one of mutual benefits: for example, providing a home for microbes enhances the body's ability to extract energy from food ‐ a form of symbiosis.},
 author = {},
 booktitle = {Understanding the Gut Microbiota},
 doi = {10.1002/9781118801413.ch3},
 keywords = {},
 pages = {19-38},
 publisher = {},
 title = {A Sense of Community},
 url = {https://nsf.dimensions.ai/details/publication/pub.1030581256},
 year = {2016}
}

@article{pub.1030581514,
 abstract = {Underground storage organs (USOs) have been proposed as critical fallback foods for early hominins in savanna, but there has been little discussion as to which habitats would have been important sources of USOs. USOs consumed by hominins could have included both underwater and underground storage organs, i.e., from both aquatic and terrestrial habitats. Shallow aquatic habitats tend to offer high plant growth rates, high USO densities, and relatively continuous USO availability throughout the year. Baboons in the Okavango delta use aquatic USOs as a fallback food, and aquatic or semiaquatic USOs support high-density human populations in various parts of the world. As expected given fossilization requisites, the African early- to mid-Pleistocene shows an association of Homo and Paranthropus fossils with shallow-water and flooded habitats where high densities of plant-bearing USOs are likely to have occurred. Given that early hominins in the tropics lived in relatively dry habitats, while others occupied temperate latitudes, ripe, fleshy fruits of the type preferred by African apes would not normally have been available year round. We therefore suggest that water-associated USOs were likely to have been key fallback foods, and that dry-season access to aquatic habitats would have been an important predictor of hominin home range quality. This study differs from traditional savanna chimpanzee models of hominin origins by proposing that access to aquatic habitats was a necessary condition for adaptation to savanna habitats. It also raises the possibility that harvesting efficiency in shallow water promoted adaptations for habitual bipedality in early hominins.},
 author = {Wrangham, Richard and Cheney, Dorothy and Seyfarth, Robert and Sarmiento, Esteban},
 doi = {10.1002/ajpa.21122},
 journal = {American Journal of Physical Anthropology},
 keywords = {},
 note = {https://dash.harvard.edu/bitstream/1/8947970/2/Text%20051409%20RW%20ES%20DASH.pdf},
 number = {4},
 pages = {630-642},
 title = {Shallow‐water habitats as sources of fallback foods for hominins},
 url = {https://nsf.dimensions.ai/details/publication/pub.1030581514},
 volume = {140},
 year = {2009}
}

@article{pub.1030607573,
 abstract = {Summary
                  The excavation of the Sweet track at two sites in the Somerset Levels has yielded unique material of the late fourth millennium bc. The track was built across a reed marsh, and the builders used a variety of timber obtained from both nearby fen-wood and from the dry land of the Burtle sand and the Poldens. Pollen analyses indicate that certain areas of the Levels were particularly favourable for forest clearance and cultivation, and the debris from the track shows a range of woodwork so far unique to the British Neolithic.},
 author = {Coles, John M. and Hibbert, F. A. and Orme, Bryony J. and Pettit, M. and Rushton, D. and Switsur, V. R.},
 doi = {10.1017/s0079497x00011683},
 journal = {Proceedings of the Prehistoric Society},
 keywords = {},
