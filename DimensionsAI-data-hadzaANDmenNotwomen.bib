@inbook{pub.1145872644,
 abstract = {The gut microbiota contributes to host health by performing roles in host nutrition, metabolism, immune modulation, pathogen colonization resistance, intestinal epithelial development, and even energy homeostasis. Though the overall health situation of the host and the contribution of intestinal micro-biota have been a popular area of research, yet there are numerous gaps to be filled and the exact mechanisms are yet to be found out. This chapter intends to provide an overview of the influence of diet on gut microbiota and interventions on the modulation of gut microbiota. The chapter provides information of various gut microbiodata, evalution, relation with diet, nutrient, and overall health benefits. The gut microbiota plays numerous roles in host metabolism and maintaining host immunity and health, elevating its status to that of a forgotten organ. Gut microbiota takes part in diverse functions including nutrient metabolism, gut barrier integrity, antimicrobial protection and immunomodulation. In nutrient metabolism, gut microbiota plays an important role. The antimicrobial substances produced by the resident microbes also inhibit the growth of pathogens. The digestion and absorption of dietary components are greatly influenced by the gut microbiota. There are the fundamental role and mutual interaction between the gut microbes and the dietary components. A change in the dietary lipid can cause modulation in the gut microbiota composition. The dietary intervention is the most cost-effective and the easiest method to manipulate the gut microbiome.},
 author = {Sudhakaran, Aparna V. and Solanki, Himanshi},
 booktitle = {Phytochemicals and Medicinal Plants in Food Design},
 doi = {10.1201/9781003150336-9},
 keywords = {},
 pages = {157-175},
 publisher = {},
 title = {Gut Microbiota—Specific Food Design},
 url = {https://nsf.dimensions.ai/details/publication/pub.1145872644},
 year = {2022}
}

