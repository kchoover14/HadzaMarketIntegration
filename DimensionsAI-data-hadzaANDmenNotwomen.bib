@inbook{pub.1040058767,
 abstract = {Flavor perception supports food choice, and it has several characteristics: (1) smell is a key part, but there is little awareness of its role; (2) variations in odorant delivery are not noticed; (3) flavor is localized to the mouth; (4) central interactions occur between all flavor senses; and (5) there is limited access to some sensory components. Together, these suggest flavor perception is partially holistic, with odor and taste forming a common sensory channel in the mouth. One reason for this mode of perception is food choice. Flavor memories can support the identification of nutritious food via: (1) recovery of the flavor percept—thus including taste and somatosensory experiences—via sniffing; (2) visual priming of flavor expectancies; and (3) affective reaction to flavor, reflecting the effects of previous bouts of ingestion (eg, sickness). These allow food acceptability to be assessed prior to ingestion, as well as supporting the detection of adulterated food.},
 author = {Stevenson, Richard J.},
 booktitle = {Flavor},
 doi = {10.1016/b978-0-08-100295-7.00008-6},
 keywords = {},
 pages = {161-180},
 publisher = {},
 title = {7 Holistic perception and memorization of flavor},
 url = {https://nsf.dimensions.ai/details/publication/pub.1040058767},
 year = {2016}
}

@article{pub.1040231289,
 abstract = {The objective of this paper is twofold. It intends to reformulate the assumptions of interpretive archaeology and within this context to elaborate the framework within which one can study social and symbolic relationships between humans and animals in the prehistoric farming communities of Central Europe. Postprocessual archaeology has failed to provide a satisfactory account of how to judge the credibility of competing issues about the past. In the interpretive approach to animal bone assemblages, it is argued that the results of actualistic studies justify this credibility. The objective of actualistic studies is to establish how, when, and by whom the archaeological record has been produced. Credibility is also provided by the biological characteristics of animals maintained and consumed. The first level of credibility is a prerequisite to a further interpretation of the social dimensions of human action with respect to animals. The second level of interpretation is based on the horizontal distribution of archaeological pasts within and between sites.},
 author = {Marciniak, Arkadiusz},
 doi = {10.1023/a:1021974930776},
