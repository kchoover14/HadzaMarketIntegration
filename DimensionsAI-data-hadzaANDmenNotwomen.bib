@inbook{pub.1127516910,
 abstract = {Even though its origins date back to 1810 together with the founding of the Mayor Staff Corp (Baldovin Ruiz 2001), the Spanish Army Geographical Service (AGS) carries out its activities along 1939–1998, years when its foundation-recentralization into the Army Geographical Center (AGC) takes place.The detailed knowledge of a territory goes through various stages where in some of them the Army becomes the main actor, such as: exploration, construction and representation. If one of these should be highlighted, it would be the latter, due to the strategic importance of getting good maps from the possible theatres of operations.All along its existence, the AGS has dedicated the mayor part of its efforts to elaborate different cartography (National Topographic Map, 1:200.000 Military map of Spain; a 1:50.000 new series L; new 1:250.00, 1:500.000 and 1:1000.000 NATO standardised cartography), carried out individually or in collaboration with other institutions (Geographic Institute).Reasons of different character: political, economic, strategic or even technological, may have influenced in the re-centralization of the AGS into AGC. The main objective of this communication is to put in value the labor carried out by the AGS in relation to the representation of the territory.},
 author = {Fraga López, Francisco Javier},
 booktitle = {Graphical Heritage},
 doi = {10.1007/978-3-030-47987-9_21},
 keywords = {},
 pages = {260-272},
 publisher = {},
 title = {Detailed Knowledge of Territory and the Spanish Army Geographical Service},
 url = {https://nsf.dimensions.ai/details/publication/pub.1127516910},
 year = {2020}
}

@article{pub.1127593068,
 abstract = {Fossil hominin footprints preserve data on a remarkably short time scale compared to most other fossil evidence, offering snapshots of organisms in their immediate ecological and behavioral contexts. Here, we report on our excavations and analyses of more than 400 Late Pleistocene human footprints from Engare Sero, Tanzania. The site represents the largest assemblage of footprints currently known from the human fossil record in Africa. Speed estimates show that the trackways reflect both walking and running behaviors. Estimates of group composition suggest that these footprints were made by a mixed-sex and mixed-age group, but one that consisted of mostly adult females. One group of similarly-oriented trackways was attributed to 14 adult females who walked together at the same pace, with only two adult males and one juvenile accompanying them. In the context of modern ethnographic data, we suggest that these trackways may capture a unique snapshot of cooperative and sexually divided foraging behavior in Late Pleistocene humans.},
 author = {Hatala, Kevin G. and Harcourt-Smith, William E. H. and Gordon, Adam D. and Zimmer, Brian W. and Richmond, Brian G. and Pobiner, Briana L. and Green, David J. and Metallo, Adam and Rossi, Vince and Liutkus-Pierce, Cynthia M.},
 doi = {10.1038/s41598-020-64095-0},
 journal = {Scientific Reports},
 keywords = {},
 note = {https://www.nature.com/articles/s41598-020-64095-0.pdf},
 number = {1},
 pages = {7740},
 title = {Snapshots of human anatomy, locomotion, and behavior from Late Pleistocene footprints at Engare Sero, Tanzania},
 url = {https://nsf.dimensions.ai/details/publication/pub.1127593068},
 volume = {10},
 year = {2020}
}

