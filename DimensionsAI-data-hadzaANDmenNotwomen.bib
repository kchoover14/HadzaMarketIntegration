@article{pub.1058309011,
 abstract = {This paper presents; an experimental examination of the potential function of Early Stone Age (ESA) stone tool technology. The discussion situates the behavior associated with ESA technology within the context of evidence for subsistence activities from faunal remains. On the basis of this experiment, I argue that: (I) many tasks associated with carcass processing in situations of secondary carcass access could have been carried out with equal efficiency with unmodified cobbles rather than modified stone tools; (2) core tools offer no major advantages over unmodified cobbles in bone breaking; and (3) flakes are generally more effective than core tools in cutting tasks such as defleshing and joint disarticulation. Therefore, I argue that defleshing and joint disarticulation were the primary uses of stone tool technology within the context off carcass processing I also propose that a great deal of early hominid carcass processing was done without modified stone tools, leaving neither lithic nor faunal evidence of these activities. Stone tools may have been infrequently required for regular subsistence activities, but they take on such importance because of their visibility relative to other remains. Finally, the paper suggests that stone tool manufacture may have emerged from a pattern of marginal scavenging that required no stone tool manufacture and left no archaeological evidence.},
 author = {McCall, Grant S.},
 doi = {10.1080/01977261.2005.11721023},
 journal = {Lithic Technology},
 keywords = {},
 number = {1},
 pages = {29-43},
 title = {An Experimental Examination of the Potential Function of Early Stone Age Tool Technology and Implications for Subsistence Behavior},
 url = {https://nsf.dimensions.ai/details/publication/pub.1058309011},
 volume = {30},
 year = {2005}
}

@article{pub.1058309074,
 abstract = {Lithic refit analysis is a time-intensive, yet rewarding, analytical tool that has the potential to yield substantial data for studies of spatial organhation, taphonomic processes, and technological organization. Few other types of analysis allow archaeologists to prove—not just hypothesize—spatial links between materials or features. Discerning what those links mean is another issue, but one cannot dispute that conjoining artifacts from different loci reflect some form of connection. Unfortunately, constraints of time and moneyfor most archaeological research projects do not typically permit time-consuming analyses such as lithic refitting, at least as it has been practiced in some of the best-known case studies, such as thePincevent site, in France (Leroi-Gourhan and Brézillon 1966; Leroi-Gourhan 1984; Pelegrin 1985, 1986; Bodu et al. 1990). This paper presents a case studyfrom the Big Eddy site in Southwest Missouri that offers insight into how we can work within budgetary and temporal constraints to produce acceptable and useful data by refitting materials for which less fine-grained spatial data are available. In this case, refit analysis of materials recoveredfrom general midden contexts within 5- and 10-cm excavation levels and from 16 lithic knapping features allowed for an assessment of the degree of integrity of the site's deposits and the spatial organization of activities performed by the site's various Paleoindian occupants.},
 author = {Stackelbeck, Kary L.},
 doi = {10.1080/01977261.2010.11721082},
 journal = {Lithic Technology},
 keywords = {},
 number = {1},
 pages = {37-62},
 title = {Maximizing the Research Potential of Refit Analysis Without Replicating Pincevent: A Case Study from the Big Eddy Site in Southwest Missouri},
 url = {https://nsf.dimensions.ai/details/publication/pub.1058309074},
 volume = {35},
 year = {2010}
}

