 journal = {Research Square},
 keywords = {},
 note = {https://www.researchsquare.com/article/rs-615147/latest.pdf},
 number = {},
 pages = {},
 title = {Characteristic and Interplay of Esophageal Microbiota in Esophageal Squamous Cell Carcinoma},
 url = {https://nsf.dimensions.ai/details/publication/pub.1138928887},
 volume = {},
 year = {2021}
}

