 volume = {45},
 year = {2022}
}

@article{pub.1152638670,
 abstract = {Actions that do not have instrumental goals can communicate social goals that are not rituals. Many non-instrumental actions such as bowing or kissing communicate a commitment to or roles in dyadic relationships. What is unclear is when people understand such actions in terms of ritual and when they understand them in terms of relationships.},
 author = {Thomas, Ashley J. and Radkani, Setayesh and Hung, Michelle S.},
 doi = {10.1017/s0140525x22001224},
 journal = {Behavioral and Brain Sciences},
 keywords = {},
 note = {https://psyarxiv.com/9jhnv/download},
 number = {},
