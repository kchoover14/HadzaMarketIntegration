 number = {399},
 pages = {709-723},
 title = {Nutritional deficiency and ecological stress in the Middle to Final western Jōmon},
 url = {https://nsf.dimensions.ai/details/publication/pub.1170347998},
 volume = {98},
 year = {2024}
}

