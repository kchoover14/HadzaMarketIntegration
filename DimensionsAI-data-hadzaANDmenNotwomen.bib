 number = {1},
 pages = {85-96},
 title = {The shaft‐based methodological approach to the quantification of long limb bones and its relevance to understanding hominid subsistence in the Pleistocene: application to four Palaeolithic sites},
 url = {https://nsf.dimensions.ai/details/publication/pub.1015543509},
 volume = {24},
 year = {2008}
}

