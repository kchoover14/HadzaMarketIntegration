}

@inbook{pub.1155293395,
 abstract = {Quantitative data – tallies, percentages, ratios – are attractive to scientists. It was near the middle of the twentieth century when archaeologists actively began to quantify the materials they recovered. In archaeobotany and zooarchaeology, there exist relatively simple counting units, some simple statistical manipulations of data, and with increases in computer power and statistical sophistication, a number of increasingly complex multivariate analytical methods. Plant remains range from macroscopic – anything not requiring a microscope to observe and identify, such as seeds, leaves, and stems – to microscopic materials such as pollen, phytoliths, and starch residues. Zooarchaeologists have something of an advantage over palaeoethnobotanists when it comes to quantification. Quantitative data in zooarchaeology and archaeobotany also facilitate intersite comparisons such that the analyst can zoom out and study patterns across space and changes over time for topics such as past diet or landscapes.},
 author = {Lyman, R. Lee and Wolverton, Steve},
 booktitle = {Handbook of Archaeological Sciences},
 doi = {10.1002/9781119592112.ch60},
 keywords = {},
 pages = {1211-1225},
 publisher = {},
 title = {Quantification in Zooarchaeology and Palaeoethno(Archaeo)botany},
 url = {https://nsf.dimensions.ai/details/publication/pub.1155293395},
 year = {2023}
}

@article{pub.1155344594,
 abstract = {Shift of ingestive behavior is an important strategy for animals to adapt to change of the environment. We knew that shifts in animal dietary habits lead to changes in the structure of the gut microbiota, but we are not sure about if changes in the composition and function of the gut microbiota respond to changes in the nutrient intake or food items. To investigate how animal feeding strategies affect nutrient intakes and thus alter the composition and digestion function of gut microbiota, we selected a group of wild primate group for the study. We quantified their diet and macronutrients intake in four seasons of a year, and instant fecal samples were analyzed by high-throughput sequencing of 16S rRNA and metagenomics. These results demonstrated that the main reason that causes seasonal shifts of gut microbiota is the macronutrient variation induced by seasonal dietary differences. Gut microbes can help to compensate for insufficient macronutrients intake of the host through microbial metabolic functions. This study contributes to a deeper understanding of the causes of seasonal variation in host-microbial variation in wild primates.},
 author = {Li, Yuhang and Yan, Yujie and Fu, Hengguang and Jin, Shiyu and He, Shujun and Wang, Zi and Dong, Guixin and Li, Baoguo and Guo, Songtao},
 doi = {10.3389/fmicb.2023.1126189},
 journal = {Frontiers in Microbiology},
 keywords = {},
 note = {https://www.frontiersin.org/articles/10.3389/fmicb.2023.1126189/pdf},
 number = {},
 pages = {1126189},
