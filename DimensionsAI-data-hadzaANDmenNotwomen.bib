@inbook{pub.1107149344,
 abstract = {This chapter attempts to describe the most commonly occurring archaeological materials that are found in thin sections of soils and archaeological deposits, with comments on their archaeological provenance, importance and relationship to the interpretation of past human activities. Such a broad treatment has not been previously attempted. Soil micromorphology is an essential tool in identifying materials that commonly occur in small sizes and amounts and is able to discriminate between materials that have been burned, digested or waterlogged, for example. The technique also ensures the context and relationship between these materials and the sediments in which they occur, which is a fundamental requisite to any understanding of an archaeological context. The chapter includes materials relating to earth-based ground-raising and construction, hearths, domestic space and animal management (organic and inorganic waste materials). Industrial processes and manufactured materials (e.g., lime plasters, metals and stone tools) are also briefly included.},
 author = {Macphail, Richard I. and Goldberg, Paul},
 booktitle = {Interpretation of Micromorphological Features of Soils and Regoliths},
 doi = {10.1016/b978-0-444-63522-8.00027-9},
 keywords = {},
 pages = {779-819},
 publisher = {},
 title = {Chapter 27 Archaeological Materials},
 url = {https://nsf.dimensions.ai/details/publication/pub.1107149344},
 year = {2018}
}

@inbook{pub.1107149722,
 abstract = {The impact of human activities on soils can be examined at many spatial scales. This chapter considers those human activities that directly influence soils and regoliths, such as agricultural practices, application of waste materials and land clearance through fire husbandry. These practices result in a wide range of anthropogenic features, which may be observed in soil and regolith thin sections, potentially enabling interpretation of both the type and nature of the practice itself. Variation in the intensity of such practices, especially contrasts between one single event and annual repetition, may lead to pronounced contrasts in the features observed between sample sites. This potentially confounds analyses and limits interpretation. By considering examples from a range of agricultural and other managed landscapes and through discussion of examples from research literature, this chapter aims to the further understanding of anthropogenic features observed for soils, allowing interpretation of a range of processes.},
 author = {Adderley, W. Paul and Wilson, Clare A. and Simpson, Ian A. and Davidson, Donald A.},
 booktitle = {Interpretation of Micromorphological Features of Soils and Regoliths},
 doi = {10.1016/b978-0-444-63522-8.00026-7},
 keywords = {},
 pages = {753-777},
 publisher = {},
 title = {Chapter 26 Anthropogenic Features},
 url = {https://nsf.dimensions.ai/details/publication/pub.1107149722},
 year = {2018}
