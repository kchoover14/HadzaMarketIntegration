@inbook{pub.1142683974,
 abstract = {Human maturation from an evolutionary and life history perspective involves study of development and maturity patterns in chimpanzees (genus Pan), the closest living relative of Homo sapiens. Methods focus on study of hard tissues – dental eruption, joint fusion, and brain size – that preserve as fossils; it is then possible to assess fossil species relative to both Pan and H. sapiens. Advances in the study of development in fossils are enhanced through microhistological analyses that utilize dental enamel secretion lines to provide an age-at-death of immature individuals. Fossils from several sites in eastern and southern Africa that date as far back as 3 million years show that species of australopiths and Homo erectus mature dental, cranial and postcranial systems similarly to wild chimpanzees. Later species such as H. neandertalensis, closely related to H. sapiens, share a maturation pattern with modern humans.},
 author = {Bolter, Debra R. and Zihlman, Adrienne L.},
 booktitle = {Human Growth and Development},
 doi = {10.1016/b978-0-12-822652-0.00016-x},
 keywords = {},
 pages = {425-448},
 publisher = {},
 title = {Chapter 15 Evolution of human growth},
 url = {https://nsf.dimensions.ai/details/publication/pub.1142683974},
 year = {2022}
}

