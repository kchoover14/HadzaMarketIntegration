 volume = {45},
 year = {2022}
}

@article{pub.1152639944,
 abstract = {Biologists have replaced the metaphor of "genetic transmission" with a detailed account of the molecular mechanisms underlying the phenomenon which Darwin referred to as "like produces like." Cultural evolution theorists, in contrast, continue to appeal to "imitation" or "copying." The notion of ritual and instrumental stances does not resolve this issue, and ignores the institutions in which people live.},
 author = {Packer, Martin J. and Cole, Michael},
 doi = {10.1017/s0140525x22001169},
 journal = {Behavioral and Brain Sciences},
 keywords = {},
 number = {},
 pages = {e264},
 title = {Bifocal stance theory, the transmission metaphor, and institutional reality},
 url = {https://nsf.dimensions.ai/details/publication/pub.1152639944},
 volume = {45},
 year = {2022}
}

@article{pub.1152640261,
 abstract = {The bifocal stance theory posits two stances - the ritual and the instrumental - each a learning strategy with different fidelity outcomes. These differences in turn have long-term consequences for cultural stability. Yet we suggest the key concept of "fidelity" is insufficiently explicated. Pointing to counterexamples and gaps in the theory, we suggest that explicating "fidelity" reveals the stances to be <i>heuristic explanatory strategies</i>: first-pass explanatory glosses of learning and its consequences, not descriptions of the inner machinery of agents.},
 author = {Buskell, Andrew and Charbonneau, Mathieu},
 doi = {10.1017/s0140525x22001194},
 journal = {Behavioral and Brain Sciences},
 keywords = {},
 number = {},
 pages = {e253},
 title = {Fidelity, stances, and explaining cultural stability},
 url = {https://nsf.dimensions.ai/details/publication/pub.1152640261},
 volume = {45},
 year = {2022}
}

@article{pub.1152640346,
 abstract = {The authors make a distinction between instrumental copying behavior in which there is a clear reward for the copying behavior and social copying (traditions) in which the rewards for copying are less clear. However, I see no reason to distinguish between the two. We are social animals, for whom copying traditions have important rewards, those of affiliation.},
 author = {Zentall, Thomas R.},
 doi = {10.1017/s0140525x22001170},
 journal = {Behavioral and Brain Sciences},
 keywords = {},
 number = {},
 pages = {e274},
 title = {Is there a need to distinguish instrumental copying behavior from traditions?},
 url = {https://nsf.dimensions.ai/details/publication/pub.1152640346},
 volume = {45},
 year = {2022}
}

@article{pub.1152640625,
 abstract = {The target article elaborates upon an extant theoretical framework, "Imitation and Innovation: The Dual Engines of Cultural Learning." We raise three major concerns: (1) There is limited discussion of cross-cultural universality and variation; (2) overgeneralization of overimitation and omission of other social learning types; and (3) selective imitation in infants and toddlers is not discussed.},
