@inbook{pub.1172823743,
 abstract = {The human digestive tract hosts a diverse array of microorganisms, collectively known as gut microbiota. This ecosystem is unique to each individual, evolving over a lifetime and susceptible to internal and external adaptations. The gut microbiome plays an important part in various aspects of human physiology, including nutrition, stress response, and behavior. Moreover, these microbes can either contribute to or cause a variety of diseases, impacting not only the digestive system but also distant organs. The intricate microbial community within this vital organ converses well with the host over neural, humoral, endocrine, metabolic, and immunological trails, regulating normal metabolism. Non-pathogenic microorganisms in the gut maintain a synergetic rapport with the immune system of the host, enhancing its ability to defend against pathogenic invaders. Dysbiosis, which involves an imbalanced gut microflora composition, can disturb the intricate equilibrium within the body. This imbalance has been directly associated with various human ailments, including obesity, inflammatory bowel disease, hypertension, diabetes, cancer, and anxiety disorders. Understanding the connection among gut microbiota, metabolic byproducts, and the host’s susceptibility to diseases plays an important role in comprehending the onset and progression of these health conditions. Our understanding of the impact of interventions aimed at managing gut microbiota, whether beneficial or detrimental, remains largely undefined. Recent global clinical trials have precisely demonstrated the association of specific microbial species and eubiosis (a state of microbial balance) with both health and disease. This chapter offers a comprehensive exploration of gut microbiota composition and their role in promoting various diseases, highlighting the multidirectional interactions between these microbes and other organs.},
 author = {Goyat, Jyoti and Bhatnagar, Barkha},
 booktitle = {Probiotics},
 doi = {10.1201/9781003452249-4},
 keywords = {},
 pages = {76-94},
 publisher = {},
 title = {Gut Microbiota and Diseases},
 url = {https://nsf.dimensions.ai/details/publication/pub.1172823743},
 year = {2024}
}

