 volume = {17},
 year = {2008}
}

@article{pub.1010950518,
 abstract = {인간의 자유의지는 철학과 심리학 영역에서 중요한 이슈이다. 인간의 도덕성을 연구해온 많은 학자들은 자유의지가 도덕적 의사결정에서 핵심적인 요소라고 주장한다. 그러나 최근 자연과학의 급속한 발전과 더불어 인간의 사고 과정이 과거 요인에 의해 결정된다는 주장이 강력하게 제기되고 있다. 이러한 결정론적 입장은 특히 뇌 과학, 사회생물학, 그리고 인간의 인지를 직접적으로 다루는 생물학 영역 등에서 인간 사고과정의 과학적 메커니즘을 규명하는 것과 밀접하게 관련되어 있다. 이와 같은 연구 결과를 바탕으로 많은 사람들은 자유의지와 인간의 자율적 도덕성은 자연과학의 연구결과에 의해 심각하게 도전 받고 있다고 간주한다. 그러나 본 연구에서는 자유의지와 인간 도덕성은 결정론적 견지에서 양립 가능함을 주장하고자 한다. 이를 위해 우선 물리학, 우주론, 인지과학과 같은 자연과학적 관점에서 결정론과 비결정론에 대한 선행 연구를 고찰할 것이다. 그런 다음, 본 연구에서는 과학 연구, 자유의지의 존재, 인간 도덕성 간의 관계에 대한 철학 및 심리학적 논의를 통해 어떻게 과학적 설명이 자유의지 및 인간의 자율적 도덕성과 양립 가능한가를 규명하고자 한다. 끝으로 본 연구에서는 과학적 연구 성과가 도덕 심리학 및 도덕교육에 어떤 기여를 할 수 있는가에 대해 제시하고자 한다.},
 author = {Jeong, Changwoo and 한혜민},
 doi = {10.18338/kojmee.2010..31.1},
 journal = {Journal of Moral & Ethics Education},
 keywords = {},
 number = {31},
 pages = {1-28},
 title = {Achievements in Natural Science, the Possibility of the Free Will and Autonomous Morality: With Consideration of Moral Psychology and its Educational Implementation},
 url = {https://nsf.dimensions.ai/details/publication/pub.1010950518},
 volume = {null},
 year = {2010}
}

