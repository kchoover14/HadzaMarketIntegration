@article{pub.1123147833,
 abstract = {
                  As economic games have spread from experimental economics to other social sciences, so too have critiques of their usefulness for drawing inferences about the “real world.” What these criticisms often miss is that games can be used to reveal individuals’ private preferences in ways that observational and interview data cannot; further, economic games can be designed such that they
                  do
                  provide insights into real-world behavior. Here, we draw on our collective experience using economic games in field contexts to illustrate how researchers can strategically alter the framing or design of economic games to draw inferences about private-world or real-world preferences. A detailed case study from coastal Colombia provides an example of the subtleties of game design and how games can be combined fruitfully with self-report data. We close with a list of concrete recommendations for how to modify economic games to better match particular research questions and research contexts.
                },
 author = {Pisor, Anne C and Gervais, Matthew M and Purzycki, Benjamin G and Ross, Cody T},
 doi = {10.7287/peerj.preprints.27355v2},
 journal = {PeerJ Preprints},
 keywords = {},
 note = {https://pure.mpg.de/pubman/item/item_3177848_6/component/file_3261433/Pisor_Preferences_Preeprint_2020.pdf},
 number = {},
 pages = {e27355v2},
 title = {Preferences and constraints: The value of economic games for studying human behavior},
 url = {https://nsf.dimensions.ai/details/publication/pub.1123147833},
 volume = {7},
 year = {2019}
}

