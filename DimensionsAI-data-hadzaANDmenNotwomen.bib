@article{pub.1072987684,
 abstract = {Malgrat que tenen diferents objectes d’estudi, totes les ciències del comportament humà tenen models de la conducta humana individual. La unitat d’aquestes ciències necessita un model comú de comportament humà individual al darrere, especificat i enriquit per satisfer les necessitats particulars de cada disciplina. No existeix aquesta unitat i no pot ser assolida fàcilment, ja que les diferents disciplines tenen models incompatibles i metodologies d’investigació dispars. Amb tot, desenvolupaments teòrics empírics i recents han creat les condicions per a la unitat de les ciències del comportament incorporant principis centrals en tots els camps i basant-se en eines teòriques (com la teoria de jocs i el model d’actor racional) i tècniques de recollida de dades (com els jocs experimentals de laboratori i sobre el terreny) que transcendeixen les fronteres disciplinàries. Aquests desenvolupaments inclouen: a ) la teoria de jocs evolutiva i conductual, que proporciona un lèxic transdisciplinari per a la comunicació i la construcció de models; b ) el model d’actor racional, ancorat en la biologia però desenvolupat per la teoria econòmica, que s’aplica en totes les disciplines del comportament humà. Aquest model tracta les accions com a instrumentals, dirigides a la satisfacció de les preferències. Malgrat això, el contingut de les preferències ha de ser determinat empíricament. A més, el model d’actor racional està basat en una noció de la consistència de les preferències que no se satisfà universalment, de manera que el seu rang d’aplicabilitat ha de determinar-se també de manera empírica; c ) els experiments controlats, que s’han infrautilitzat en la major part de les ciències del comportament. La teoria de jocs i el model d’actor racional poden utilitzar-se com a base per formular, desplegar i analitzar dades generades a partir d’experiments controlats amb subjectes humans.  },
 author = {Gintis, Herbert},
 doi = {10.5565/rev/papers/v80n0.1771},
 journal = {Papers. Revista de Sociologia},
 keywords = {},
 note = {https://ddd.uab.cat/pub/papers/02102862n80/02102862n80p97.pdf},
 number = {0},
 pages = {97-122},
 title = {Towards a Unity of the Human Behavioral Sciences},
 url = {https://nsf.dimensions.ai/details/publication/pub.1072987684},
 volume = {80},
