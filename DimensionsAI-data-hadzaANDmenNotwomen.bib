@article{pub.1172457037,
 abstract = {Nous interrogeons la notion de paysage à partir d’une enquête sur les modes d’observation d’une œuvre de Giuseppe Penone. En nous penchant sur la perception des œuvres d’art au cours de la visite des salles d’un musée, nous nous situons dans l’étude de la perception en action. Alors que la notion de paysage a servi de point de repère pour dire la relation à la nature élaborée en Europe autour de la perspective géométrique, nous avons souhaité la réinterroger en rompant avec le tournant linguistique : nous abordons le paysage sur le plan des mouvements, de l’action et de la sensorialité et dans les pas du tournant corporel. Cela conduit à appréhender le paysage à la façon d’« arrangements hétéroclites » constamment renouvelés par l’expérience. Comme nous avons pu l’observer au cours de l’enquête, faire émerger un paysage est une activité qui mobilise certains savoir-faire.},
 author = {Cerclet, Denis},
 doi = {10.4000/11rck},
