@article{pub.1034812498,
 abstract = {Almost unnoticed by sociologists, experimental economics has expanded its research to many classical sociological topics such as the evolution and stability of social norms, social sanctions and social order, reciprocity, and pro-social behaviour. This article reports the main empirical findings and theories, their strengths and weaknesses, and explores how experimental economics has furthered social theory. It concludes with the suggestion that sociology should bring experimental methods back into the field and that economics should acknowledge that the research program is an interdisciplinary task analyzing the basic aspects of human behaviour.},
