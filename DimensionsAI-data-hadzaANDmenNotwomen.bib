 title = {Non-instrumental actions can communicate roles and relationships, not just rituals},
 url = {https://nsf.dimensions.ai/details/publication/pub.1152638670},
 volume = {45},
 year = {2022}
}

@article{pub.1152638693,
 abstract = {What inductive biases must be incorporated into multi-agent artificial intelligence models to get them to capture high-fidelity imitation? We think very little is needed. In the right environments, both instrumental- and ritual-stance imitation can emerge from generic learning mechanisms operating on non-deliberative decision architectures. In this view, imitation emerges from trial-and-error learning and does not require explicit deliberation.},
 author = {Leibo, Joel Z. and Köster, Raphael and Vezhnevets, Alexander Sasha and Duénez-Guzmán, Edgar A. and Agapiou, John P. and Sunehag, Peter},
 doi = {10.1017/s0140525x22001364},
 journal = {Behavioral and Brain Sciences},
