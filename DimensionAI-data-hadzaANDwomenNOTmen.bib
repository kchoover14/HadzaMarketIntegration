 abstract = {We investigated the relationship between the gut microbiome composition and diet of rural Amazonian riverines (AMZ) (n=49) and compared them to urban São Paulo dwellers (SP) (n=55). Diet was measured using...},
 author = {Cestonaro, Talita},
