 number = {1},
 pages = {1},
 title = {Paternal Exercise Improves the Metabolic Health of Offspring via Epigenetic Modulation of the Germline},
 url = {https://nsf.dimensions.ai/details/publication/pub.1144099371},
 volume = {23},
 year = {2021}
}

