@article{pub.1035482636,
 abstract = {The microbiota of the gastrointestinal tract plays an important role in human health. In addition to their metabolic interactions with dietary constituents, gut bacteria may also be involved in more complex host interactions, such as modulation of the immune system. Furthermore, the composition of the gut microbiota may be important in reducing the risk of contracting particular gut infections. Changes in the microbiota during an individual's lifespan are accompanied by modifications in multiple health parameters, and such observations have prompted intense scientific efforts aiming to understand the complex interactions between the microbiota and its human host, as well as how this may be influenced by diet.},
 author = {Milani, C. and Ferrario, C. and Turroni, F. and Duranti, S. and Mangifesta, M. and van Sinderen, D. and Ventura, M.},
