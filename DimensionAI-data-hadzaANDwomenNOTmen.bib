@inbook{pub.1014023636,
 abstract = {Menopause, the universal midlife cessation of reproductive ability in females, is unique to humans. In all other primate species the vast majority of females continue to reproduce until the end of their lives. There are three possible explanations for the perplexing evolution of reproductive cessation long before the end of the lifespan: the antagonistic pleiotropy or trade‐off hypothesis; the physiological constraint hypothesis; and the grandmother hypothesis. That the mammalian female reproductive system might be physiologically constrained to function for only fifty to fifty‐two years, despite the evolution of much longer lifespan in humans, is quite likely. However there is also some evidence that nonreproductive human grandmothers can improve the fitness of descendants by provisioning them with food—a situation that is simply too rare in nonhuman primates to have favored the evolution of menopause.},
 author = {Fedigan, Linda Marie and Pavelka, Mary S. M.},
 booktitle = {The International Encyclopedia of Human Sexuality},
