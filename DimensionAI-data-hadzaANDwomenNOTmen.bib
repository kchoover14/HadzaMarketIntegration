 year = {2008}
}

@inbook{pub.1031540219,
 abstract = {In this chapter, I provide an overview of the evolutionary approach to educational psychology and begin with a discussion of evolved biologically primary domains of the human mind and the cognitive systems that enable the creation of evolutionarily novel biologically secondary abilities (e.g., reading and writing) and knowledge (see also Geary, 1995, 2000, 2005, 2007, 2008, in press ). I then flesh out the distinction between primary and secondary abilities and learning as these relate to the creation of modern culture, outline the implications for the discipline of evolutionary educational psychology, illustrate how the corresponding framework can help us better understand children's academic learning and motivation to learn, and close with a discussion of potential bridges from primary to secondary learning. (PsycInfo Database Record (c) 2022 APA, all rights reserved)},
